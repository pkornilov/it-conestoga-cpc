<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ANPKDisplayBooksServlet</servlet-name>
        <servlet-class>club.admin.ANPKDisplayBooksServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ANPKAddBookServlet</servlet-name>
        <servlet-class>club.admin.ANPKAddBookServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ANPKLoanServlet</servlet-name>
        <servlet-class>club.cart.ANPKLoanServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ANPKCartServlet</servlet-name>
        <servlet-class>club.cart.ANPKCartServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ANPKClearCartServlet</servlet-name>
        <servlet-class>club.cart.ANPKClearCartServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ANPKMemberAdminController</servlet-name>
        <servlet-class>club.admin.ANPKMemberAdminController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ANPKDisplayBooksServlet</servlet-name>
        <url-pattern>/ANPKDisplayBooks</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ANPKAddBookServlet</servlet-name>
        <url-pattern>/ANPKAddBook</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ANPKLoanServlet</servlet-name>
        <url-pattern>/ANPKLoan</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ANPKCartServlet</servlet-name>
        <url-pattern>/ANPKCart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ANPKClearCartServlet</servlet-name>
        <url-pattern>/ANPKClearCart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ANPKMemberAdminController</servlet-name>
        <url-pattern>/ANPKMemberAdmin</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>ANPKIndex.jsp</welcome-file>
    </welcome-file-list>
    <error-page>
        <exception-type>java.lang.Throwable</exception-type>
        <location>/ANPKError.jsp</location>
    </error-page>
</web-app>
